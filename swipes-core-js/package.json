{
  "name": "@swipesapp/core",
  "version": "2.0.0-beta.15",
  "description": "The core connector for Swipes SDK JS",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist/",
