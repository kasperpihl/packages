    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "react": "^16.6.3",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.1.0",
    "immutable": "^4.0.0-rc.12",
    "memoize-one": "^4.1.0",
    "react-redux": "^5.1.0",
